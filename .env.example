# Real Estate Investment Platform Configuration
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# ATTOM Data API Configuration (optional - will use mock data if not provided)
ATTOM_API_KEY=your_attom_api_key_here

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
MAX_CONCURRENT_ANALYSES=5

# Agent Configuration
ANALYSIS_ENGINE_TIMEOUT=300
CUSTOMER_AGENT_TIMEOUT=30
DEAL_FINDER_TIMEOUT=600

# Rate Limiting
ATTOM_REQUESTS_PER_MINUTE=120
GEMINI_REQUESTS_PER_MINUTE=60

# Cache Settings
PROPERTY_CACHE_HOURS=1
MARKET_CACHE_HOURS=6